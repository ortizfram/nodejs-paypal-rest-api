
    <h3>Are you sure you want to delete blog: <%= blog.title %>?</h3>
    <form method="POST" action="<%= action %>" >
        <input type="hidden" name="_method" value="DELETE">
        <input type="hidden" name="courseId" value="<%= blog.id %>">
        <input type="submit" value="Delete">
    </form>
    <a href="/api/blog">Cancel</a>
